# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_ACCESS_KEY_ID=your-access-key-id
VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key
VITE_AWS_ACCOUNT_ID=your-aws-account-id

# S3 Buckets
VITE_AWS_S3_BUCKET=educonnect-uploads
VITE_AWS_WEBSITE_BUCKET=educonnect-website

# DynamoDB Tables
VITE_AWS_DYNAMODB_USERS_TABLE=educonnect-users
VITE_AWS_DYNAMODB_GROUPS_TABLE=educonnect-groups
VITE_AWS_DYNAMODB_MESSAGES_TABLE=educonnect-messages
VITE_AWS_DYNAMODB_ASSIGNMENTS_TABLE=educonnect-assignments
VITE_AWS_DYNAMODB_COURSES_TABLE=educonnect-courses
VITE_AWS_DYNAMODB_NOTIFICATIONS_TABLE=educonnect-notifications

# Lambda Configuration
VITE_AWS_LAMBDA_ROLE_ARN=arn:aws:iam::your-account-id:role/lambda-execution-role

# Cognito (These will be populated after running the deployment script)
VITE_AWS_COGNITO_USER_POOL_ID=
VITE_AWS_COGNITO_CLIENT_ID=
VITE_AWS_COGNITO_DOMAIN=

# API Gateway (This will be populated after deployment)
VITE_AWS_API_GATEWAY_URL=

# SNS/SQS
VITE_ADMIN_EMAIL=admin@example.com

# AWS Rekognition for face analysis
VITE_AWS_REKOGNITION_ENABLED=true
VITE_AWS_REKOGNITION_COLLECTION_ID=educonnect-faces

# AWS Lex for chatbot
VITE_AWS_LEX_BOT_ID=
VITE_AWS_LEX_BOT_ALIAS_ID=

# Deployment Configuration
VITE_GITHUB_REPO_URL=https://github.com/your-username/educonnect
VITE_GITHUB_ACCESS_TOKEN=your-github-token

# Application Settings
VITE_APP_NAME=EduConnect
VITE_DEFAULT_LANGUAGE=en 